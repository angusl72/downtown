<div class="cards">
    <% @images.each do |image| %>
      <div class="card">
        <% unless image.before_photo.key == nil %>
        <%= cl_image_tag image.before_photo.key, width: 300, height: 200, crop: :fill, gravity: "auto", class: "card-img-top"%>
        <% else %>
        <%= cl_image_tag "prvluvb2arm6lhic2on9", width: 300, height: 200, crop: :fill, gravity: "auto", class: "card-img-top" %>
        <% end %>
      </div>
    <% end %>
</div>
