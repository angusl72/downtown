<% @images.each do |image| %>
  <div class="container mb-5">
    <div class="cards">
      <div class="card">
        <% unless image.before_photo.key == nil %>
        <%= cl_image_tag image.before_photo.key, width: 285, height: 290, crop: :fill, gravity: "auto", class: "card-img-top"%>
        <%= cl_image_tag image.after_photo.key, width: 285, height: 290, crop: :fill, gravity: "auto", class: "card-img-top"%>
        <% else %>
        <%= cl_image_tag "prvluvb2arm6lhic2on9", width: 300, height: 300, crop: :fill, gravity: "auto", class: "card-img-top" %>
        <% end %>
      </div>
    </div>
  </div>
<% end %>
